{
  "name" : "Jfix the web",
  "manifest_version" : 2,
  "version" : "1.0",
  "description" : "Fixes various sites",
  "content_scripts" : [
    {
      "matches" : ["http://www.sluggy.com/comics/*"],
      "css" : ["sluggy/sluggy.css"]
    },
    {
      "matches" : ["https://readms.net/*"],
      "css" : ["mangastream/mangastream.css"]
    },
    {
      "matches" : ["http://weathernews.jp/*"],
      "css" : ["weathernews/weathernews.css"]
    },
    {
      "matches" : ["https://screenshot.googleplex.com/*"],
      "css" : ["snipit/snipit.css"]
    },
    {
      "matches" : ["https://mail.google.com/*"],
      "css" : ["gmail/gmail.css"]
//      "js" : ["gmail/gmail.js"],
//      "run_at" : "document_start"
    },
    {
      "matches" : ["https://hangouts.google.com/webchat/*", "https://apis.google.com/*/hovercard*"],
      "css" : ["gmail/chat.css"],
      "all_frames" : true
    },
    {
      "matches" : ["http://minecraft.gamepedia.com/*"],
      "css" : ["minecraft_wiki/minecraft_wiki.css"]
    },
    {
      "matches" : ["http://www.canalplus.fr/*"],
      "css" : ["canalplus/canalplus.css"],
      "js" : ["canalplus/canalplus.js"]
    },
    {
      "matches" : ["https://www.facebook.com/*"],
      "css" : ["facebook/facebook.css"]
    },
    {
      "matches" : ["https://www.youtube.com/*"],
      "css" : ["youtube/youtube.css"]
    },
    {
      "matches" : ["https://drive.google.com/*", "https://youtube.googleapis.com/embed/*"],
      "css" : ["googledrive/googledrive.css"],
      "all_frames" : true
    },
    {
      "matches" : ["https://photos.google.com/*"],
      "css" : ["googlephotos/googlephotos.css"]
    },
    {
      "matches" : ["https://www.google.co.jp/search*", "https://www.google.com/search*"],
      "css" : ["googlesearch/googlesearch.css"]
    },
    {
      "matches" : ["https://www.google.co.jp/maps/*", "https://www.google.com/maps/*"],
      "css" : ["maps/maps.css"]
    },
    {
      "matches" : ["http://battlecats.spica-net.com/*"],
      "css" : ["battlecats/battlecats.css"]
    },
    {
      "matches" : ["http://www.750g.com/*"],
      "css" : ["750g/750g.css"]
    },
    {
      "matches" : ["http://www.starpowercomic.com/*"],
      "css" : ["starpower/starpower.css"]
    },
    {
      "matches" : ["https://www.shadertoy.com/*"],
      "css" : ["shadertoy/shadertoy.css"]
    },
    {
      "matches" : ["https://www.dumbingofage.com/*"],
      "css" : ["dumbingofage/dumbingofage.css"]
    },
    {
      "matches" : ["https://parahumans.wordpress.com/*"],
      "css" : ["worm/worm.css"]
    },
    {
      "matches" : ["https://*.fandom.com/*"],
      "css" : ["fandom/fandom.css"]
    }
  ]
}
