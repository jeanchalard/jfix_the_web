{
  "name" : "Jfix the web",
  "manifest_version" : 3,
  "version" : "1.0",
  "description" : "Fixes various sites",
  "content_scripts" : [
    { // Websites using the didomi sdk
      "matches" : ["https://*/*"],
      "css" : ["didomi/didomi.css"],
      "js" : ["didomi/didomi.js"]
    },
    {
      "matches" : ["http://www.sluggy.com/comics/*"],
      "css" : ["sluggy/sluggy.css"]
    },
    {
      "matches" : ["https://discord.com/*"],
      "css" : ["discord/discord.css"]
    },
    {
      "matches" : ["http://weathernews.jp/*"],
      "css" : ["weathernews/weathernews.css"]
    },
    {
      "matches" : ["https://screenshot.googleplex.com/*"],
      "css" : ["snipit/snipit.css"]
    },
// No longer needed since chrome has auto-dark mode
//    {
//      "matches" : ["https://mail.google.com/*"],
//      "css" : ["gmail/gmail.css"]
// Was disabled before
//      "js" : ["gmail/gmail.js"],
//      "run_at" : "document_start"
//    },
    {
      "matches" : ["https://hangouts.google.com/webchat/*", "https://apis.google.com/*/hovercard*"],
      "css" : ["gmail/chat.css"],
      "all_frames" : true
    },
    {
      "matches" : ["http://minecraft.gamepedia.com/*"],
      "css" : ["minecraft_wiki/minecraft_wiki.css"]
    },
    {
      "matches" : ["http://www.canalplus.fr/*"],
      "css" : ["canalplus/canalplus.css"],
      "js" : ["canalplus/canalplus.js"]
    },
    {
      "matches" : ["https://www.facebook.com/*"],
      "css" : ["facebook/facebook.css"]
    },
    {
      "matches" : ["https://www.instagram.com/*"],
      "css" : ["instagram/instagram.css"]
    },
    {
      "matches" : ["https://www.youtube.com/*"],
      "css" : ["youtube/youtube.css"]
    },
    {
      "matches" : ["https://drive.google.com/*", "https://youtube.googleapis.com/embed/*"],
      "css" : ["googledrive/googledrive.css"],
      "all_frames" : true
    },
    {
      "matches" : ["https://docs.google.com/spreadsheets/*"],
      "css" : ["googlesheets/googlesheets.css"]
    },
    {
      "matches" : ["https://photos.google.com/*"],
      "css" : ["googlephotos/googlephotos.css"]
    },
    {
      "matches" : ["https://www.google.co.jp/search*", "https://www.google.com/search*"],
      "css" : ["googlesearch/googlesearch.css"]
    },
    {
      "matches" : ["https://www.google.co.jp/maps/*", "https://www.google.com/maps/*"],
      "css" : ["maps/maps.css"]
    },
    {
      "matches" : ["http://battlecats.spica-net.com/*"],
      "css" : ["battlecats/battlecats.css"]
    },
    {
      "matches" : ["http://www.750g.com/*"],
      "css" : ["750g/750g.css"]
    },
    {
      "matches" : ["http://www.starpowercomic.com/*"],
      "css" : ["starpower/starpower.css"]
    },
    {
      "matches" : ["https://www.shadertoy.com/*"],
      "css" : ["shadertoy/shadertoy.css"]
    },
    {
      "matches" : ["https://www.dumbingofage.com/*"],
      "css" : ["dumbingofage/dumbingofage.css"]
    },
    {
      "matches" : ["https://www.smbc-comics.com/*"],
      "css" : ["smbc/smbc.css"]
    },
    {
      "matches" : ["http://superredundant.com/*"],
      "css" : ["losrh/losrh.css"]
    },
    {
      "matches" : ["https://genshin-impact-map.appsample.com/*"],
      "css" : ["genshinmap/genshinmap.css"]
    },
    {
      "matches" : ["https://parahumans.wordpress.com/*"],
      "css" : ["worm/worm.css"]
    },
    {
      "matches" : ["https://*.fandom.com/*"],
      "css" : ["fandom/fandom.css"]
    },
    {
      "matches" : ["https://www.dailymotion.com/*"],
      "css" : ["dailymotion/dailymotion.css"]
    },
    {
      "matches" : ["https://genshin-impact-map.appsample.com/*"],
      "css" : ["genshin/genshin.css"]
    },
    {
      "matches" : ["https://www.skyscanner.jp/*", "https://www.skyscanner.fr/*", "https://www.skyscanner.com/*"],
      "css" : ["skyscanner/skyscanner.css"]
    },
    {
      "matches" : ["https://analysebrassens.com/*", "https://www.analysebrassens.com/*", "http://analysebrassens.com/*", "http://www.analysebrassens.com/*"],
      "css" : ["analysebrassens/analysebrassens.css"]
    },
    {
      "matches" : ["https://*.stackexchange.com/*", "https://wiki.ubuntu.com/*"],
      "css" : ["stackexchange/stackexchange.css"]
    },
    {
      "matches" : ["https://www.lemonde.fr/*"],
      "css" : ["lemonde/lemonde.css"]
    },
    {
      "matches" : ["https://www.lefigaro.fr/*"],
      "css" : ["figaro/figaro.css"]
    },
    {
      "matches" : ["https://www.liberation.fr/*"],
      "css" : ["liberation/liberation.css"]
    },
    {
      "matches" : ["https://www.cuisineactuelle.fr/*"],
      "css" : ["cuisineactuelle/cuisineactuelle.css"]
    },
    {
      "matches" : ["https://www.cuisineaz.com/*"],
      "css" : ["cuisineaz/cuisineaz.css"]
    },
    {
      "matches" : ["https://leasticoulddo.com/*"],
      "css" : ["leasticoulddo/leasticoulddo.css"]
    },
    {
      "matches" : ["https://www.larousse.fr/*"],
      "js" : ["larousse/larousse.js"],
      "css" : ["larousse/larousse.css"]
    },
    {
      "matches" : ["https://trackings.post.japanpost.jp/*"],
      "css" : ["japanpost/japanpost.css"]
    },
    {
      "matches" : ["https://member.rakuten-sec.co.jp/*"],
      "css" : ["rakuten_shoken/rakuten_shoken.css"]
    },
    {
      "matches" : ["https://superredundant.com/*"],
      "css" : ["losrh/losrh.css"]
    },
    {
      "matches" : ["https://no1.nipponrentacar.co.jp/*"],
      "css" : ["nipponrentacar/nipponrentacar.css"],
      "all_frames" : true,
      "match_about_blank" : true,
      "match_origin_as_fallback" : true
    },
    {
      "matches" : ["https://www.reddit.com/*"],
      "css" : ["reddit/reddit.css"]
    },
    {
      "matches" : ["https://www.jalan.net/*", "http://www.jalan.net/*"],
      "css" : ["jalan/jalan.css"]
    },
    {
      "matches" : ["https://questionablecontent.net/*"],
      "css" : ["questionablecontent/questionablecontent.css"]
    }
  ]
}
